          {
            "group": "Platform",
            "pages": [
              "platform/overview",
              "platform/playground",
              "platform/developer-platform"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
